{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport './SeatChart.css';\nexport default {\n  name: \"SeatChartPage\",\n  data() {\n    return {\n      searchQuery: '',\n      seatingCharts: [],\n      isSeatDialogVisible: false,\n      seatForm: {\n        floorNo: '',\n        seatNo: '',\n        isOccupied: false\n      },\n      isEdit: false,\n      currentSeat: null,\n      isConfirmDialogVisible: false,\n      seatToDelete: null\n    };\n  },\n  computed: {\n    filteredSeats() {\n      return this.seatingCharts.filter(seat => seat.floorNo && seat.floorNo.toString().includes(this.searchQuery) || seat.seatNo && seat.seatNo.toString().includes(this.searchQuery));\n    }\n  },\n  methods: {\n    loadSeatingCharts() {\n      this.$apiService.get(this.$seatingCharts.getAllSeatingCharts).then(res => {\n        this.seatingCharts = res;\n      }).catch(() => {});\n    },\n    showAddSeatDialog() {\n      this.isSeatDialogVisible = true;\n      this.isEdit = false;\n      this.seatForm = {\n        floorNo: '',\n        seatNo: '',\n        isOccupied: false\n      };\n    },\n    showEditSeatDialog(seat) {\n      this.isSeatDialogVisible = true;\n      this.isEdit = true;\n      this.currentSeat = seat;\n      this.seatForm = {\n        ...seat\n      };\n    },\n    submitSeatForm() {\n      if (this.isEdit) {\n        this.$apiService.put(this.$seatingCharts.updateSeatingChart(this.currentSeat.floorSeatSeq), this.seatForm).then(() => {\n          const index = this.seatingCharts.findIndex(s => s.floorSeatSeq === this.currentSeat.floorSeatSeq);\n          if (index !== -1) {\n            this.seatingCharts[index] = {\n              ...this.seatForm\n            };\n          }\n          alert(\"座位更新成功！\");\n          this.closeDialog();\n        }).catch(error => {\n          console.error(\"Error updating seat:\", error);\n          alert(\"更新座位失敗！\");\n        });\n      } else {\n        this.$apiService.post(this.$seatingCharts.createSeatingChart, this.seatForm).then(res => {\n          this.seatingCharts.push(res);\n          alert(\"座位新增成功！\");\n          this.closeDialog();\n        }).catch(error => {\n          console.error(\"Error adding seat:\", error);\n          alert(\"新增座位失敗！\");\n        });\n      }\n    },\n    closeDialog() {\n      this.isSeatDialogVisible = false;\n      this.seatForm = {\n        floorNo: '',\n        seatNo: '',\n        isOccupied: false\n      };\n      this.currentSeat = null;\n    },\n    confirmDelete(seat) {\n      this.seatToDelete = seat;\n      this.isConfirmDialogVisible = true;\n    },\n    closeConfirmDialog() {\n      this.isConfirmDialogVisible = false;\n      this.seatToDelete = null;\n    },\n    deleteSeat() {\n      this.$apiService.delete(this.$seatingCharts.deleteSeatingChart(this.seatToDelete.floorSeatSeq)).then(() => {\n        const index = this.seatingCharts.indexOf(this.seatToDelete);\n        if (index !== -1) {\n          this.seatingCharts.splice(index, 1);\n        }\n        alert(\"座位已刪除！\");\n        this.closeConfirmDialog();\n      }).catch(error => {\n        console.error(\"Error deleting seat:\", error);\n        alert(\"刪除座位失敗！\");\n        this.closeConfirmDialog();\n      });\n    }\n  },\n  created() {\n    this.loadSeatingCharts();\n  }\n};","map":{"version":3,"names":["name","data","searchQuery","seatingCharts","isSeatDialogVisible","seatForm","floorNo","seatNo","isOccupied","isEdit","currentSeat","isConfirmDialogVisible","seatToDelete","computed","filteredSeats","filter","seat","toString","includes","methods","loadSeatingCharts","$apiService","get","$seatingCharts","getAllSeatingCharts","then","res","catch","showAddSeatDialog","showEditSeatDialog","submitSeatForm","put","updateSeatingChart","floorSeatSeq","index","findIndex","s","alert","closeDialog","error","console","post","createSeatingChart","push","confirmDelete","closeConfirmDialog","deleteSeat","delete","deleteSeatingChart","indexOf","splice","created"],"sources":["C:\\Users\\tzutung\\employee-seating\\employee-seating-frontend\\src\\views\\SeatChart\\SeatChart.vue"],"sourcesContent":["<template>\r\n  <div class=\"seating-container\">\r\n    <h1>座位資料管理</h1>\r\n    <input type=\"text\" v-model=\"searchQuery\" placeholder=\"請輸入樓層或座位號\" class=\"search-input\" />\r\n    <button @click=\"showAddSeatDialog\" class=\"btn add-btn\">\r\n      <i class=\"pi pi-plus\"></i> 新增座位\r\n    </button>\r\n    <table class=\"seating-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>樓層</th>\r\n          <th>座位號</th>\r\n          <th>佔用狀態</th>\r\n          <th>操作</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"seat in filteredSeats\" :key=\"seat.floorSeatSeq\">\r\n          <td>{{ seat.floorNo }}</td>\r\n          <td>{{ seat.seatNo }}</td>\r\n          <td>{{ seat.isOccupied ? '佔用' : '空閒' }}</td>\r\n          <td>\r\n            <button @click=\"showEditSeatDialog(seat)\" class=\"btn edit-btn\">\r\n              <i class=\"pi pi-pencil\"></i> 編輯\r\n            </button>\r\n            <button @click=\"deleteSeat(seat)\" class=\"btn delete-btn\">\r\n              <i class=\"pi pi-trash\"></i> 刪除\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <div v-if=\"isConfirmDialogVisible\" class=\"smodal\">\r\n      <div class=\"smodal-content\">\r\n        <h3>確認刪除</h3>\r\n        <p>您確定要刪除這個座位嗎？</p>\r\n        <div class=\"button-group\">\r\n          <button @click=\"deleteSeat\" class=\"modal-btn confirm-btn\"><i class=\"pi pi-check\"></i> 確定</button>\r\n          <button @click=\"closeConfirmDialog\" class=\"modal-btn cancel-btn\"><i class=\"pi pi-times\"></i> 取消</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-if=\"isSeatDialogVisible\" class=\"smodal\">\r\n      <div class=\"smodal-content\">\r\n        <h3>{{ isEdit ? '編輯座位' : '新增座位' }}</h3>\r\n        <label>樓層:</label>\r\n        <input type=\"number\" v-model=\"seatForm.floorNo\" placeholder=\"輸入樓層\" />\r\n        <label>座位:</label>\r\n        <input type=\"number\" v-model=\"seatForm.seatNo\" placeholder=\"輸入座位號\" />\r\n        <label>狀態:</label>\r\n        <input type=\"text\" value=\"未占用\" disabled />\r\n        <div class=\"button-group\">\r\n          <button @click=\"submitSeatForm\" class=\"modal-btn confirm-btn\"><i class=\"pi pi-check\"></i>送出</button>\r\n          <button @click=\"closeDialog\" class=\"modal-btn cancel-btn\"><i class=\"pi pi-times\"></i>取消</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport './SeatChart.css';\r\n\r\nexport default {\r\n  name: \"SeatChartPage\",\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      seatingCharts: [],\r\n      isSeatDialogVisible: false,\r\n      seatForm: { floorNo: '', seatNo: '', isOccupied: false },\r\n      isEdit: false,\r\n      currentSeat: null,\r\n      isConfirmDialogVisible: false,\r\n      seatToDelete: null,\r\n    };\r\n  },\r\n  computed: {\r\n    filteredSeats() {\r\n      return this.seatingCharts.filter(seat =>\r\n        (seat.floorNo && seat.floorNo.toString().includes(this.searchQuery)) ||\r\n        (seat.seatNo && seat.seatNo.toString().includes(this.searchQuery))\r\n      );\r\n    }\r\n  },\r\n  methods: {\r\n    loadSeatingCharts() {\r\n      this.$apiService\r\n        .get(this.$seatingCharts.getAllSeatingCharts)\r\n        .then((res) => {\r\n          this.seatingCharts = res;\r\n        })\r\n        .catch(() => {});\r\n    },\r\n\r\n    showAddSeatDialog() {\r\n      this.isSeatDialogVisible = true;\r\n      this.isEdit = false;\r\n      this.seatForm = { floorNo: '', seatNo: '', isOccupied: false };\r\n    },\r\n\r\n    showEditSeatDialog(seat) {\r\n      this.isSeatDialogVisible = true;\r\n      this.isEdit = true;\r\n      this.currentSeat = seat;\r\n      this.seatForm = { ...seat };\r\n    },\r\n\r\n    submitSeatForm() {\r\n      if (this.isEdit) {\r\n        this.$apiService\r\n          .put(this.$seatingCharts.updateSeatingChart(this.currentSeat.floorSeatSeq), this.seatForm)\r\n          .then(() => {\r\n            const index = this.seatingCharts.findIndex(s => s.floorSeatSeq === this.currentSeat.floorSeatSeq);\r\n            if (index !== -1) {\r\n              this.seatingCharts[index] = { ...this.seatForm };\r\n            }\r\n            alert(\"座位更新成功！\");\r\n            this.closeDialog();\r\n          })\r\n          .catch((error) => {\r\n            console.error(\"Error updating seat:\", error);\r\n            alert(\"更新座位失敗！\");\r\n          });\r\n      } else {\r\n        this.$apiService\r\n          .post(this.$seatingCharts.createSeatingChart, this.seatForm)\r\n          .then((res) => {\r\n            this.seatingCharts.push(res);\r\n            alert(\"座位新增成功！\");\r\n            this.closeDialog();\r\n          })\r\n          .catch((error) => {\r\n            console.error(\"Error adding seat:\", error);\r\n            alert(\"新增座位失敗！\");\r\n          });\r\n      }\r\n    },\r\n\r\n    closeDialog() {\r\n      this.isSeatDialogVisible = false;\r\n      this.seatForm = { floorNo: '', seatNo: '', isOccupied: false };\r\n      this.currentSeat = null;\r\n    },\r\n\r\n    confirmDelete(seat) {\r\n      this.seatToDelete = seat;\r\n      this.isConfirmDialogVisible = true;\r\n    },\r\n\r\n    closeConfirmDialog() {\r\n      this.isConfirmDialogVisible = false;\r\n      this.seatToDelete = null;\r\n    },\r\n\r\n    deleteSeat() {\r\n      this.$apiService\r\n        .delete(this.$seatingCharts.deleteSeatingChart(this.seatToDelete.floorSeatSeq))\r\n        .then(() => {\r\n          const index = this.seatingCharts.indexOf(this.seatToDelete);\r\n          if (index !== -1) {\r\n            this.seatingCharts.splice(index, 1);\r\n          }\r\n          alert(\"座位已刪除！\");\r\n          this.closeConfirmDialog();\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error deleting seat:\", error);\r\n          alert(\"刪除座位失敗！\");\r\n          this.closeConfirmDialog();\r\n        });\r\n    }\r\n  },\r\n  created() {\r\n    this.loadSeatingCharts();\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";;;AA+DA,OAAO,iBAAiB;AAExB,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,EAAE;MACjBC,mBAAmB,EAAE,KAAK;MAC1BC,QAAQ,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,MAAM,EAAE,EAAE;QAAEC,UAAU,EAAE;MAAM,CAAC;MACxDC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE,IAAI;MACjBC,sBAAsB,EAAE,KAAK;MAC7BC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACX,aAAa,CAACY,MAAM,CAACC,IAAG,IACjCA,IAAI,CAACV,OAAM,IAAKU,IAAI,CAACV,OAAO,CAACW,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAAChB,WAAW,CAAC,IAClEc,IAAI,CAACT,MAAK,IAAKS,IAAI,CAACT,MAAM,CAACU,QAAQ,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAAChB,WAAW,CAClE,CAAC;IACH;EACF,CAAC;EACDiB,OAAO,EAAE;IACPC,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACC,WAAU,CACZC,GAAG,CAAC,IAAI,CAACC,cAAc,CAACC,mBAAmB,EAC3CC,IAAI,CAAEC,GAAG,IAAK;QACb,IAAI,CAACvB,aAAY,GAAIuB,GAAG;MAC1B,CAAC,EACAC,KAAK,CAAC,MAAM,CAAC,CAAC,CAAC;IACpB,CAAC;IAEDC,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACxB,mBAAkB,GAAI,IAAI;MAC/B,IAAI,CAACK,MAAK,GAAI,KAAK;MACnB,IAAI,CAACJ,QAAO,GAAI;QAAEC,OAAO,EAAE,EAAE;QAAEC,MAAM,EAAE,EAAE;QAAEC,UAAU,EAAE;MAAM,CAAC;IAChE,CAAC;IAEDqB,kBAAkBA,CAACb,IAAI,EAAE;MACvB,IAAI,CAACZ,mBAAkB,GAAI,IAAI;MAC/B,IAAI,CAACK,MAAK,GAAI,IAAI;MAClB,IAAI,CAACC,WAAU,GAAIM,IAAI;MACvB,IAAI,CAACX,QAAO,GAAI;QAAE,GAAGW;MAAK,CAAC;IAC7B,CAAC;IAEDc,cAAcA,CAAA,EAAG;MACf,IAAI,IAAI,CAACrB,MAAM,EAAE;QACf,IAAI,CAACY,WAAU,CACZU,GAAG,CAAC,IAAI,CAACR,cAAc,CAACS,kBAAkB,CAAC,IAAI,CAACtB,WAAW,CAACuB,YAAY,CAAC,EAAE,IAAI,CAAC5B,QAAQ,EACxFoB,IAAI,CAAC,MAAM;UACV,MAAMS,KAAI,GAAI,IAAI,CAAC/B,aAAa,CAACgC,SAAS,CAACC,CAAA,IAAKA,CAAC,CAACH,YAAW,KAAM,IAAI,CAACvB,WAAW,CAACuB,YAAY,CAAC;UACjG,IAAIC,KAAI,KAAM,CAAC,CAAC,EAAE;YAChB,IAAI,CAAC/B,aAAa,CAAC+B,KAAK,IAAI;cAAE,GAAG,IA