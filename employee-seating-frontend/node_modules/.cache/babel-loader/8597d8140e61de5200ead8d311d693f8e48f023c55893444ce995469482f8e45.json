{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport './SeatChart.css';\nexport default {\n  name: \"SeatChartPage\",\n  // 改成多單詞命名\n  data() {\n    return {\n      seatingCharts: [],\n      newSeat: {\n        floorNo: 1,\n        seatNo: 1,\n        isOccupied: false\n      }\n    };\n  },\n  methods: {\n    addSeat() {\n      this.seatingCharts.push({\n        ...this.newSeat\n      });\n      this.newSeat = {\n        floorNo: 1,\n        seatNo: 1,\n        isOccupied: false\n      };\n    },\n    editSeat(seat) {\n      alert(`編輯座位: ${seat.floorNo}樓, 座位${seat.seatNo}`);\n    },\n    deleteSeat(seat) {\n      const index = this.seatingCharts.indexOf(seat);\n      if (index !== -1) {\n        this.seatingCharts.splice(index, 1);\n      }\n    }\n  },\n  created() {\n    this.seatingCharts = [{\n      floorSeatSeq: 1,\n      floorNo: 1,\n      seatNo: 1,\n      isOccupied: true\n    }, {\n      floorSeatSeq: 2,\n      floorNo: 1,\n      seatNo: 2,\n      isOccupied: false\n    }];\n  }\n};","map":{"version":3,"names":["name","data","seatingCharts","newSeat","floorNo","seatNo","isOccupied","methods","addSeat","push","editSeat","seat","alert","deleteSeat","index","indexOf","splice","created","floorSeatSeq"],"sources":["C:\\Users\\tzutung\\employee-seating\\employee-seating-frontend\\src\\views\\SeatChart\\SeatChart.vue"],"sourcesContent":["<template>\r\n  <div class=\"seating-container\">\r\n    <h1>座位資料管理</h1>\r\n    <button @click=\"addSeat\" class=\"btn add-btn\">新增座位</button>\r\n    <table class=\"seating-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>樓層</th>\r\n          <th>座位號</th>\r\n          <th>佔用狀態</th>\r\n          <th>操作</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"seat in seatingCharts\" :key=\"seat.floorSeatSeq\">\r\n          <td>{{ seat.floorNo }}</td>\r\n          <td>{{ seat.seatNo }}</td>\r\n          <td>{{ seat.isOccupied ? '佔用' : '空閒' }}</td>\r\n          <td>\r\n            <button @click=\"editSeat(seat)\" class=\"btn edit-btn\">編輯</button>\r\n            <button @click=\"deleteSeat(seat)\" class=\"btn delete-btn\">刪除</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport './SeatChart.css';\r\n\r\nexport default {\r\n  name: \"SeatChartPage\",  // 改成多單詞命名\r\n  data() {\r\n    return {\r\n      seatingCharts: [],\r\n      newSeat: { floorNo: 1, seatNo: 1, isOccupied: false }\r\n    };\r\n  },\r\n  methods: {\r\n    addSeat() {\r\n      this.seatingCharts.push({ ...this.newSeat });\r\n      this.newSeat = { floorNo: 1, seatNo: 1, isOccupied: false };\r\n    },\r\n    editSeat(seat) {\r\n      alert(`編輯座位: ${seat.floorNo}樓, 座位${seat.seatNo}`);\r\n    },\r\n    deleteSeat(seat) {\r\n      const index = this.seatingCharts.indexOf(seat);\r\n      if (index !== -1) {\r\n        this.seatingCharts.splice(index, 1);\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.seatingCharts = [\r\n      { floorSeatSeq: 1, floorNo: 1, seatNo: 1, isOccupied: true },\r\n      { floorSeatSeq: 2, floorNo: 1, seatNo: 2, isOccupied: false }\r\n    ];\r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":";AA6BA,OAAO,iBAAiB;AAExB,eAAe;EACbA,IAAI,EAAE,eAAe;EAAG;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,OAAO,EAAE;QAAEC,OAAO,EAAE,CAAC;QAAEC,MAAM,EAAE,CAAC;QAAEC,UAAU,EAAE;MAAM;IACtD,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR,IAAI,CAACN,aAAa,CAACO,IAAI,CAAC;QAAE,GAAG,IAAI,CAACN;MAAQ,CAAC,CAAC;MAC5C,IAAI,CAACA,OAAM,GAAI;QAAEC,OAAO,EAAE,CAAC;QAAEC,MAAM,EAAE,CAAC;QAAEC,UAAU,EAAE;MAAM,CAAC;IAC7D,CAAC;IACDI,QAAQA,CAACC,IAAI,EAAE;MACbC,KAAK,CAAC,SAASD,IAAI,CAACP,OAAO,QAAQO,IAAI,CAACN,MAAM,EAAE,CAAC;IACnD,CAAC;IACDQ,UAAUA,CAACF,IAAI,EAAE;MACf,MAAMG,KAAI,GAAI,IAAI,CAACZ,aAAa,CAACa,OAAO,CAACJ,IAAI,CAAC;MAC9C,IAAIG,KAAI,KAAM,CAAC,CAAC,EAAE;QAChB,IAAI,CAACZ,aAAa,CAACc,MAAM,CAACF,KAAK,EAAE,CAAC,CAAC;MACrC;IACF;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAACf,aAAY,GAAI,CAC